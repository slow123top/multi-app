  "version": "0.2.0",
  "configurations": [
    {
      "name": "npm install",
      "request": "launch",
      "runtimeArgs": ["run-script", "install"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "npm test",
      "request": "launch",
      "runtimeArgs": ["run-script", "build:test"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "npm bind",
      "request": "launch",
      "runtimeArgs": ["run-script", "build:bind"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "npm promise",
      "request": "launch",
      "runtimeArgs": ["run-script", "build:promise"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}"
    },
    {
      "name": "webpack vue",
      "request": "launch",
      "runtimeArgs": ["run-script", "serve"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "build vue",
      "request": "launch",
      "runtimeArgs": ["run-script", "build"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    }
  ]
}
